{
  "name": "nestjs-swagger-sync",
  "version": "4.3.1",
  "description": "NestJS plugin to sync Swagger documentation with Postman",
  "author": "garseta yusuf",
  "private": false,
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "nestjs",
    "swagger",
    "postman",
    "sync",
    "openapi"
  ],
  "peerDependencies": {
    "@nestjs/common": ">=6.0.0",
    "@nestjs/core": ">=6.0.0",
    "reflect-metadata": ">=0.1.13",
    "rxjs": ">=6.0.0"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "newman": "^5.3.2",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": ">=6.0.0",
    "@nestjs/common": ">=6.0.0",
    "@nestjs/core": ">=6.0.0",
    "@nestjs/schematics": ">=6.0.0",
    "@nestjs/testing": ">=6.0.0",
    "@types/jest": "^27.0.0",
    "@types/node": "^16.0.0",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.0.0",
    "prettier": "^2.0.0",
    "rimraf": "^3.0.0",
    "rxjs": ">=6.0.0",
    "ts-jest": "^27.0.0",
    "ts-loader": "^9.0.0",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.0.0",
    "typescript": "^4.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/garsetayusuf/nestjs-swagger-sync"
  },
  "bugs": {
    "url": "https://github.com/garsetayusuf/nestjs-swagger-sync/issues"
  },
  "homepage": "https://github.com/garsetayusuf/nestjs-swagger-sync#readme"
}
